# MySQL Database Configuration
# Copy this file to .env and customize as needed

# Database Configuration
MYSQL_DATABASE=myapp
MYSQL_USER=user
MYSQL_PASSWORD=changeme
MYSQL_ROOT_PASSWORD=changeme

# Ports
MYSQL_PORT=3306
ADMINER_PORT=8080

# Memory Configuration (Realistic for 16-32GB systems)
MYSQL_MEMORY_LIMIT=1g
ADMINER_MEMORY_LIMIT=256m

# Environment Labels
ENVIRONMENT=dev
TEAM=platform
COMPOSE_PROJECT_NAME=mysql-db