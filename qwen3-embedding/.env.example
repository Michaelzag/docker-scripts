# Qwen3 Embedding Service Configuration - MAXIMUM PERFORMANCE RTX 5080
# Copy this file to .env and customize the values below

# Service Configuration
QWEN3_EMBEDDING_PORT=7800
QWEN3_EMBEDDING_MODEL=Qwen/Qwen3-Embedding-4B
QWEN3_EMBEDDING_GPU_MEM=0.5
QWEN3_EMBEDDING_MAX_LEN=8192
QWEN3_EMBEDDING_DIMENSIONS=2560
QWEN3_EMBEDDING_MEMORY_LIMIT=8g

# vLLM Configuration
QWEN3_ENABLE_PREFIX_CACHING=true
QWEN3_MAX_NUM_BATCHED_TOKENS=32768
QWEN3_MAX_NUM_SEQS=256

# Authentication
QWEN3_API_KEY=sk-dummy-key

# Environment Labels
ENVIRONMENT=dev
TEAM=ai
COMPOSE_PROJECT_NAME=qwen3-embedding