{
  "mcpServers": {
    "qdrant-db": {
      "command": "uvx",
      "args": [
        "mcp-server-qdrant"
      ],
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "COLLECTION_NAME": "mcp_memory",
        "EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2"
      },
      "disabled": false,
      "alwaysAllow": [
        "qdrant-store",
        "qdrant-find"
      ]
    }
  }
}